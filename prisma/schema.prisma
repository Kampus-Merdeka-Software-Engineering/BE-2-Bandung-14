// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


model Customers {
  id      Int      @id @default(autoincrement())
  first_name   String?
  last_name    String?
  check_in String?
  check_out String?
  adults    Int?
  children  Int?
  room      String?
  address   String?
  mobile_number String?
  bukti_TF String?
  
}